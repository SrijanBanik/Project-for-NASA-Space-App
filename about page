                width;

            //  get screen width
            width = $( window ).width();

            //  determine which "display" value to use
            if ( width < 768 ) {
                display = that.settings.get( "xs" );
            } else if ( width < 992 ) {
                display = that.settings.get( "sm" );
            } else if ( width < 1200 ) {
                display = that.settings.get( "md" );
            } else {
                display = that.settings.get( "lg" );
            }

